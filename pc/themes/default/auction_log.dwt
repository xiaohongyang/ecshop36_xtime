{if $auction_log}
{foreach from=$auction_log item=log}
<dl class="corder-pre {if $log.user_id eq $user_info.user_id}active{/if}">
    <dt><img src="{$log.avatar}"></dt>
    <dd style="margin-top:20px">出价人：{$log.user_name}</dd>
    <dd>出价时间：{$log.bid_time}</dd>
    <dd class="hui">出价金额：{$log.formated_bid_price}</dd>
</dl>
{/foreach}
{else}
<h4>暂无出价记录</h4>
{/if}